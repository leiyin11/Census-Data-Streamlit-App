# Census Data Streamlit App Configuration
# Copy this file to .env and customize as needed

# Census API Configuration
CENSUS_API_YEAR=2018

# Streamlit Server Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
STREAMLIT_SERVER_HEADLESS=false

# Streamlit Theme (optional)
# STREAMLIT_THEME_PRIMARY_COLOR=#FF6B6B
# STREAMLIT_THEME_BACKGROUND_COLOR=#FFFFFF
# STREAMLIT_THEME_SECONDARY_BACKGROUND_COLOR=#F0F2F6
# STREAMLIT_THEME_TEXT_COLOR=#262730

# Debug Mode
DEBUG=false

# Data Cache Configuration (in seconds)
CACHE_TTL=3600
